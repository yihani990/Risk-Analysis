   "source": [
    "## 2. Parametric approach"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 50,
   "id": "b411e1cc",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The 95% VaR under Parametric VaR model is: 936463.4962222223\n"
     ]
    }
   ],
   "source": [
    "mean = (daily_change.mean()*ak).sum()\n",
    "variance = np.array(ak).dot(daily_change.cov()).dot(np.array(ak))\n",
    "\n",
    "z = norm.ppf(0.95) #计算出分位数\n",
    "VaR95_para = abs(mean + z*np.sqrt(variance)) #95%下的分位数\n",
    "\n",
    "print(\"The 95% VaR under Parametric VaR model is:\",VaR95_para)"
   ]
