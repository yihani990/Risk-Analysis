   "source": [
    "## 3.Mento Carlo VaR"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 51,
   "id": "86eb65b4",
   "metadata": {},
   "outputs": [],
   "source": [
    "num_simulations = 10000\n",
    "cov_matrix_a=daily_change.cov()\n",
    "mean_a = daily_change.mean()\n",
    "simulated_L = np.random.multivariate_normal(mean_a, cov_matrix_a, size=num_simulations)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 52,
   "id": "30992fdc",
   "metadata": {},
   "outputs": [],
   "source": [
    "sofr_simulated = []\n",
    "stock_simulated = []\n",
    "for row in simulated_L:\n",
    "    first_ten_elements = row[:10]\n",
    "    last_four_elements = row[10:14]\n",
    "    sofr_simulated.append(first_ten_elements)\n",
    "    stock_simulated.append(last_four_elements)"
   ]
  },
