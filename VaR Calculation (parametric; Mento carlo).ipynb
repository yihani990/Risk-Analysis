   "source": [
    "a=[]\n",
    "for i in range(len(SOFR_a1['2023-10-30'])):\n",
    "    temp_df = SOFR_a1.copy()    \n",
    "    temp_df.at[temp_df.index[i], '2023-10-30'] += 1e-4   \n",
    "    temp_df['DF_flt2'] = 1 / (1 + temp_df['2023-10-30'])**temp_df['T']\n",
    "    flt=1-temp_df['DF_flt2'].iloc[-1]\n",
    "    fixed = sum(temp_df['DF_flt2']) * 0.042\n",
    "    VALUE = (flt- fixed) * 1e8\n",
    "    a.append((VALUE - swap_value)*1e4)\n",
    "ak = a+[1e6]*4\n",
    "ak"
   ]
