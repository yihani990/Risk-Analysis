   "source": [
    "#### -- Calculate a(k)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 48,
   "id": "0eb31fb6",
   "metadata": {},
   "outputs": [],
   "source": [
    "SOFR_a1 = SOFR[['T','2023-10-30']].iloc[6:16]\n",
    "SOFR_a1['DF_flt']=1/(1+SOFR_a1['2023-10-30'])**SOFR_a1['T']\n",
    "DF_FIX= 0.042*sum(SOFR_a1['DF_flt'])\n",
    "swap_value = (1-SOFR_a1['DF_flt'].iloc[-1]-DF_FIX)*1e8"
   ]
  },
